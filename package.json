{
    "name": "@voces/wc3maptranslator",
    "version": "4.0.0",
    "description": "Module to translate between `war3map` and `json` formats for WarCraft III .w3x maps",
    "keywords": [
        "wc3",
        "warcraft",
        "reforged",
        "frozen throne",
        "reign of chaos",
        "war3map",
        "translator",
        "json",
        "w3x"
    ],
    "main": "index.js",
    "scripts": {
        "build": "tsc --build",
        "lint": "tslint --project .",
        "test": "npm run build && mocha",
        "test-nyc": "npm run build && nyc mocha",
        "test-travis": "npm run build && ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --include-all-sources true --root ./lib -- -R spec ./test/*.js"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://voces@github.com/voces/WC3MapTranslator.git"
    },
    "author": "ChiefOfGxBxL",
    "contributors": [
        "rufreakde"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/voces/WC3MapTranslator/issues"
    },
    "homepage": "https://github.com/voces/WC3MapTranslator#readme",
    "dependencies": {
        "intn": "^1.0.0",
        "round-to": "^4.1.0"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^0.1.3",
        "@types/fs-extra": "^8.0.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.26",
        "@types/round-to": "^4.0.0",
        "coveralls": "^3.0.9",
        "diff-buf": "^1.0.1",
        "fs-extra": "^8.1.0",
        "ieee754": "^1.1.13",
        "istanbul": "^0.4.5",
        "mocha": "^6.2.2",
        "nyc": "^14.1.1",
        "source-map-support": "^0.5.16",
        "ts-node": "^8.6.2",
        "tslint": "^5.20.1",
        "typescript": "^3.7.5"
    }
